{
    "env": {
        "es6": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "ignorePatterns": ["node_modules/"],
    "parserOptions": {
        "project": true
    },
    "root": true,
    "rules": {
        "import/no-anonymous-default-export": "off",
        "no-console": "warn",
        "no-control-regex": "off",
        "no-unused-vars": "off"
    }
}
